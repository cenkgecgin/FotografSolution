@model IEnumerable<Teslim>

<a class="btn btn-primary" asp-action="Create">Yeni Teslim</a>
<table class="table table-hover">
    <thead>
        <tr>
            <td>@Html.DisplayNameFor(p=>p.Id)</td>
            <td>@Html.DisplayNameFor(p=>p.TeslimEdenAdi)</td>
            <td>@Html.DisplayNameFor(p=>p.TeslimEdenTel)</td>
            <td>@Html.DisplayNameFor(p=>p.Images)</td>



            <td></td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.TeslimEdenAdi</td>
                <td>@item.TeslimEdenTel</td>
                <td>@item.Images</td>


                <td>
                    <a class="btn btn-danger" asp-action="Delete" asp-controller="Teslim" asp-route-id="@item.Id">Sil</a>
                    <a class="btn btn-info" asp-action="Update" asp-controller="Teslim" asp-route-id="@item.Id">Güncelle</a>
                </td>
            </tr>
        }
    </tbody>

</table>